<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FabriikToolbar">
        <attr name="title" format="string" />
        <attr name="tintColor" format="color" />
        <attr name="showBack" format="boolean" />
        <attr name="showTitle" format="boolean" />
        <attr name="showDismiss" format="boolean" />
        <attr name="showInfo" format="boolean" />
    </declare-styleable>

    <declare-styleable name="FabriikSwitch">
        <attr name="leftOptionText" format="string" />
        <attr name="rightOptionText" format="string" />
        <attr name="selectedOption" format="enum">
            <enum name="none" value="0" />
            <enum name="left" value="1" />
            <enum name="right" value="2" />
        </attr>
    </declare-styleable>
</resources>